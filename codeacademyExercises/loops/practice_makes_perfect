#Practice Makes Perfect

pokemon = [ "Pikachu", "Charmander", "Squirtle" ]

print "Which pokemon you want?"

for i in pokemon:
    if i == "Pikachu":
        print " Pika pika Pika"
    else:
        print " I am not Pikachu"

#Define a function is_even that will take a number x as input.
#If x is even, then return True.
#Otherwise, return False.

def is_even(x):
    if x % 2 == 0:
        return True
    else:
        return False